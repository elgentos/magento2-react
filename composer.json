{
  "name": "elgentos/magento2-react",
  "type": "magento2-component",
  "version": "0.0.1",
  "license": "OSL-3.0",
  "description": "Module to add React to Magento 2 frontend",
  "keywords": [
    "composer-installer",
    "magento",
    "react",
    "webpack"
  ],
  "require": {
    "magento/framework": "^100.1|^101.0|^102.0|^103.0"
  },
  "require-dev": {
    "composer/composer": "*@dev"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Elgentos\\React\\": ""
    }
  },
  "extra": {
    "map": [
      [
        "example/webpack.config.js",
        "webpack.config.js.sample"
      ],
      [
        "example/package.json",
        "package.json.sample"
      ]
    ]
  }
}
